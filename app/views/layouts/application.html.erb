<!DOCTYPE html>
<html>
<head>
  <title>TexArch</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render partial: "/shared/navbar" %>
  <div class="container">
    <% flash.each do |type, message| %>
      <div class=""><h5><%= content_tag :div, message.html_safe, class:"flash-#{type}" %></h5></div>
    <% end %>

    <%= yield %>
  </div>

</body>
</html>

<h2>All Users</h2>
<div class="row">

  <div class="col-md-11">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>ID</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
          <th>First Visit</th>
        </tr>
      </thead>
      <tbody>
      <% @users.each do |user| %>
        <tr>
          <td> <%= user.id %></td>
          <td> <%= user.first_name %></td>
          <td> <%= user.last_name %></td>
          <td> <%= user.email %></td>
          <td> <%= user.first_visit %></td>
        </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>
